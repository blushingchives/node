FROM ubuntu:22.04

# GO version
ARG GO_VERSION=1.19.7
# kujira-core version
ARG BINARY_VERSION=v0.8.7

WORKDIR root

RUN apt update && apt dist-upgrade -y

RUN apt install build-essential git unzip curl wget lz4 jq -y

# remove old go version
RUN rm -rvf /usr/local/go/

# download recent go version
RUN wget https://go.dev/dl/go1.19.7.linux-amd64.tar.gz

# install go
RUN tar -C /usr/local -xzf go1.19.7.linux-amd64.tar.gz

# remove unneeded installer
RUN rm go1.19.7.linux-amd64.tar.gz

# source go
ENV GOROOT=/usr/local/go
ENV GOPATH=$HOME/go
ENV GO111MODULE=on
ENV PATH=$PATH:/usr/local/go/bin:$HOME/go/bin

# RUN git clone https://github.com/Team-Kujira/core $HOME/kujira-core
# WORKDIR $HOME/kujira-core
# RUN git checkout v0.8.7

# clone kujira-core
RUN git clone https://github.com/Team-Kujira/core $HOME/kujira-core

# checkout to current tag
RUN cd $HOME/kujira-core && git checkout ${BINARY_VERSION}

# install binary
RUN cd $HOME/kujira-core && make install

ARG CHAIN_ID=kaiyo-1
ARG MONIKER_NAME=node1

RUN kujirad init ${MONIKER_NAME} --chain-id ${CHAIN_ID}

RUN sed -i "s/^minimum-gas-prices *=.*/minimum-gas-prices = \"0.00119ukuji,0.00150factory\/kujira1qk00h5atutpsv900x202pxx42npjr9thg58dnqpa72f2p7m2luase444a7\/uusk,0.00150ibc\/295548A78785A1007F232DE286149A6FF512F180AF5657780FC89C009E2C348F,0.000125ibc\/27394FB092D2ECCD56123C74F36E4C1F926001CEADA9CA97EA622B25F41E5EB2,0.00126ibc\/47BD209179859CDE4A2806763D7189B6E6FE13A17880FE2B42DE1E6C1E329E23,0.00652ibc\/3607EB5B5E64DD1C0E12E07F077FF470D5BC4706AFCBC98FE1BA960E5AE4CE07,617283951ibc\/F3AA7EF362EC5E791FE78A0F4CCC69FEE1F9A7485EB1A8CAB3F6601C00522F10,0.000288ibc\/EFF323CC632EC4F747C61BCE238A758EFDB7699C3226565F7C20DA06509D59A5,0.000125ibc\/DA59C009A0B3B95E0549E6BF7B075C8239285989FF457A8EDDBB56F10B2A6986,0.00137ibc\/A358D7F19237777AF6D8AD0E0F53268F8B18AE8A53ED318095C14D6D7F3B2DB5,0.0488ibc\/4F393C3FCA4190C0A6756CE7F6D897D5D1BE57D6CCB80D0BC87393566A7B6602,78492936ibc\/004EBF085BBED1029326D56BE8A2E67C08CECE670A94AC1947DF413EF5130EB2,964351ibc\/1B38805B1C75352B28169284F96DF56BDEBD9E8FAC005BDCC8CF0378C82AA8E7\"/;" $HOME/.kujira/config/app.toml
RUN sed -i "s/^timeout_commit *=.*/timeout_commit = \"1500ms\"/;" $HOME/.kujira/config/config.toml
RUN sed -i "s/^query_gas_limit *=.*/query_gas_limit = \"100000000\"/;" $HOME/.kujira/config/app.toml
RUN sed -i "s|laddr = \"tcp://127.0.0.1:26657\"|laddr = \"tcp://0.0.0.0:26657\"|;" $HOME/.kujira/config/config.toml


