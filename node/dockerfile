RUN RPC=https://rpc-kujira.mintthemoon.xyz:443 && LATEST_HEIGHT=$(curl -s ${RPC}/block | jq -r .result.block.header.height) && TRUST_HEIGHT=$((LATEST_HEIGHT - 2000)) && TRUST_HASH=$(curl -s ${RPC}/block?height=${TRUST_HEIGHT} | jq -r .result.block_id.hash) && sed -i.bak -E "s|^(enable[[:space:]]+=[[:space:]]+).*$|\1true| ; \
s|^(rpc_servers[[:space:]]+=[[:space:]]+).*$|\1\"$RPC,$RPC\"| ; \
s|^(trust_height[[:space:]]+=[[:space:]]+).*$|\1$TRUST_HEIGHT| ; \
